 */


public class CarDesign 
{
	
	//Class variables or Global variables
	String name;
	int price;
	String model;
	int wheels;
	
	public void start()
	{
		System.out.println("Car starts");
	}
	
	public void stop() 
	{
		System.out.println("Car stops");
	}
	
	public void run() 
	{
		System.out.println("Car runs");
	}
	
	public static void main(String[] args) 
	{

	}

}
