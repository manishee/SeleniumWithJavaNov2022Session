package OOP_Encapsulation;

/**
 * 
 * @author mchugh
 * This Program is about Encapsulation and data hiding.
 */
public class Employee {

	String name;
	int id;
	int age;
	double salary;
	
	public static void main(String[] args) 
	{
		Employee e1=new Employee();
		e1.name="Tom";
		e1.age=23;
		System.out.println(e1.name);
	}

}
