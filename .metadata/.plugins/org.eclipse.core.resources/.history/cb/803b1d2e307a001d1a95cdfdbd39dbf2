package SeleniumSessions;

public class Person {
	
	String name;
	int age;
	char gender;
	
	public static void main(String[] args) {
		
		Person p1=new Person();
		p1.name="Kiran";
		p1.age=30;
		p1.gender='m';

		Person p2=new Person();
		p1.name="Priya";
		p1.age=40;
		p1.gender='f';
		
		System.out.println(p1.name + " " + p1.age + " " +p1.gender);
		System.out.println(p2.name + " " + p2.age + " " +p2.gender);

		
		
	}

}
